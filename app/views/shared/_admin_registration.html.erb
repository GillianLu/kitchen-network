<%= form_for(admin, as: resource_name, url: create_admin_registration_path) do |f| %>
    <%= render 'devise/shared/error_messages', resource: admin %>

    <div class="field">
        <%= f.label :first_name %>
        <%= f.text_field :first_name %>
    </div>

    <div class="field">
        <%= f.label :last_name %>
        <%= f.text_field :last_name %>
    </div>

    <div class="field">
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div>

    <div class="field">
        <%= f.label :phone_number %>
        <%= f.text_field :phone_number %>
    </div>

    <div class="field">
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>

    <div class="field">
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation %>
    </div>

    <div class="actions">
        <%= f.submit "Sign up" %>
    </div>
<% end %>
